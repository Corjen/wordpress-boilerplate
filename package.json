{
  "name": "wordpress-gulp-webpack-composer-starter-pack",
  "version": "1.2.0",
  "description": "Starter pack for wordpress theme development. Includes gulp (with production build), webpack, composer, critical css, unit testing, linting, etc..",
  "main": "gulpconfig.js",
  "scripts": {
    "clean:dist": "rm -rf dist && mkdir dist",
    "clean:build": "rm -rf public/wp-content/themes && mkdir public/wp-content/themes",
    "dev": "webpack --watch --progress",
    "build": "npm run test && npm run clean:dist && npm run build:gulp && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack -p --config webpack.config.production.js --progress",
    "build:gulp": "gulp build-production",
    "start": "npm run clean:build && gulp init && gulp",
    "release": "standard-version && git push --follow-tags && git push && git push --tags"
  },
  "repository": "git+https://github.com/Corjen/wordpress-gulp-webpack-composer-starter-pack.git",
  "author": "Corjen Moll",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Corjen/wordpress-gulp-webpack-composer-starter-pack/issues"
  },
  "homepage": "https://github.com/Corjen/wordpress-gulp-webpack-composer-starter-pack#readme",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.14.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-changed": "^3.0.0",
    "gulp-changed-in-place": "^2.0.3",
    "gulp-cheerio": "^0.6.2",
    "gulp-clean-css": "^3.0.4",
    "gulp-hash": "^4.1.0",
    "gulp-imagemin": "^3.0.3",
    "gulp-load-plugins": "^1.2.4",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sass-glob-import": "^0.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgo": "^1.0.3",
    "gulp-svgstore": "^6.0.0",
    "gulp-watch": "^4.3.9",
    "jsonfile": "^4.0.0",
    "prettier": "^1.7.4",
    "require-dir": "^0.3.0",
    "script-loader": "^0.7.0",
    "standard": "^10.0.2",
    "standard-version": "^4.0.0",
    "svgstore": "^2.0.2",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "fontfaceobserver": "^2.0.9",
    "hyperform": "^0.9.5",
    "raven-js": "^3.14.2"
  }
}
